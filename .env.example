# Server Configuration
# Copy this file to .env and fill in your values

# Environment: development or production
# - development: deploys to /home/ubuntu/tests/SERVER_NAME
# - production: deploys to /home/ubuntu/servers/SERVER_NAME
ENVIRONMENT="development"

# Server name
SERVER_NAME="less_east_c1"
PARTY_NAME="Teams @ Less Test"

# Server port
PORT="6666"
# SSH Configuration
SSH_HOST="ubuntu@ec2-10-10-10-10.us-east-2.compute.amazonaws.com"
SSH_KEY="~/.ssh/yourkey.pem"
